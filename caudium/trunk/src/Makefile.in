SRCDIR=@srcdir@
VPATH=@srcdir@
prefix=@prefix@

@SET_MAKE@

DIRS="@DIRS@"

all: $(DIRS)

force:
	@:

cmods: force
	( cd $@ && $(MAKE) $(MAKE_FLAGS))
tools: force
	( cd $@ && $(MAKE) $(MAKE_FLAGS))

install: force
	for a in $(DIRS); do ( cd $$a && $(MAKE) $(MAKE_FLAGS) install); done
clean: force
	for a in $(DIRS); do ( cd $$a && $(MAKE) $(MAKE_FLAGS) clean); done
depend: force
	for a in $(DIRS); do ( cd $$a && $(MAKE) $(MAKE_FLAGS) depend); done



